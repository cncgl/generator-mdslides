    "coffeelint": "^1.14.2",
    "coffeelint-use-strict": "^1.0.0",
    "coveralls": "^2.11.9",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-coffee": "^0.13.0",
    "grunt-contrib-watch": "^0.6.1",
    "istanbul": "^0.4.0",
